{
  "name": "airbnb-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for Airbnb location lookup with REST API integration and filtering",
  "main": "build/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc && node build/index.js",
    "start": "node build/index.js",
    "clean": "rm -rf build",
    "watch": "tsc --watch"
  },
  "keywords": [
    "mcp",
    "airbnb",
    "rest-api",
    "travel",
    "accommodation"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.18.1",
    "node-fetch": "^3.3.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
